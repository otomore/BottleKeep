<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- App Store Connect配信用の設定 -->
    <key>method</key>
    <string>app-store-connect</string>

    <!-- TestFlight配信を有効化 -->
    <key>uploadBitcode</key>
    <false/>

    <!-- シンボル情報をアップロード -->
    <key>uploadSymbols</key>
    <true/>

    <!-- 自動的に証明書とプロビジョニングプロファイルを管理 -->
    <key>manageAppVersionAndBuildNumber</key>
    <true/>

    <!-- Team ID -->
    <key>teamID</key>
    <string>KRVLW3Y2SL</string>

    <!-- Bundle ID -->
    <key>bundleIdentifier</key>
    <string>com.bottlekeep.whiskey</string>

    <!-- コード署名設定 -->
    <key>signingStyle</key>
    <string>manual</string>

    <!-- TestFlight専用設定 -->
    <key>destination</key>
    <string>export</string>

    <!-- コンパイル時の設定 -->
    <key>compileBitcode</key>
    <false/>

    <!-- 署名証明書の設定 -->
    <key>signingCertificate</key>
    <string>Apple Distribution</string>

    <!-- プロビジョニングプロファイル指定 -->
    <key>provisioningProfiles</key>
    <dict>
        <key>com.bottlekeep.whiskey</key>
        <string>BottleKeeper Distribution</string>
    </dict>

    <!-- App Store Connect API認証 -->
    <key>apiKeyPath</key>
    <string>~/private_keys/AuthKey_3HFGXYNO7U2Q.p8</string>

    <key>apiKeyID</key>
    <string>3HFGXYNO7U2Q</string>

    <key>apiKeyIssuerId</key>
    <string>69a6de80-2c1a-47e3-e053-5b8c7c11a4d1</string>
</dict>
</plist>